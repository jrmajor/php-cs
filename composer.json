{
    "name": "jrmajor/cs",
    "description": "My PHP-CS-Fixer configuration",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremiasz Major",
            "email": "jrh.mjr@gmail.com"
        }
    ],
    "homepage": "https://github.com/jrmajor/php-cs",
    "require": {
        "php": ">= 7.4",
        "friendsofphp/php-cs-fixer": "^3.8",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [
            "src/Rules/helpers.php",
            "src/config.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Major\\CS\\Tests\\": "tests/"
        }
    }
}
