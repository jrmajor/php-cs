{
    "name": "jrmajor/cs",
    "description": "My PHP-CS-Fixer configuration",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremiasz Major",
            "email": "jrh.mjr@gmail.com"
        }
    ],
    "homepage": "https://github.com/jrmajor/php-cs",
    "require": {
        "php": "8.2 - 8.4",
        "friendsofphp/php-cs-fixer": "^3.64",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.22"
    },
    "require-dev": {
        "php-cs-fixer/phpunit-constraint-isidenticalstring": "^1.5",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^11.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Major\\CS\\": "src/"
        },
        "files": [
            "src/Rules/helpers.php",
            "src/config.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Major\\CS\\Tests\\": "tests/"
        }
    }
}
