{
    "name": "jrmajor/cs",
    "description": "My PHP-CS-Fixer configuration",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremiasz Major",
            "email": "jrh.mjr@gmail.com"
        }
    ],
    "homepage": "https://github.com/jrmajor/php-cs",
    "require": {
        "php": "8.0 - 8.2",
        "friendsofphp/php-cs-fixer": "^3.23",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.16"
    },
    "require-dev": {
        "php-cs-fixer/phpunit-constraint-isidenticalstring": "^1.2",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Major\\CS\\": "src/"
        },
        "files": [
            "src/Rules/helpers.php",
            "src/config.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Major\\CS\\Tests\\": "tests/"
        }
    }
}
